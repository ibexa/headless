{
    "name": "ibexa/headless",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Headless edition",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://updates.ibexa.co"
        }
    ],
    "replace": {
        "ibexa/content": "*"
    },
    "require": {
        "php": ">=8.3",
        "ibexa/oss": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/connect": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/measurement": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/migrations": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/image-editor": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/oauth2-client": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/installer": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/product-catalog": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/seo": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/scheduler": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/calendar": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/connector-dam": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/elasticsearch": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/fastly": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/icons": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/personalization": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/version-comparison": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/workflow": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/taxonomy": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/tree-builder": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/content-tree": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/image-picker": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/site-context": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/headless-assets": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/app-switcher": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "gregwar/captcha-bundle": "^2.1"
    },
    "require-dev": {
        "ibexa/ci-scripts": "^0.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
